{
  "expo": {
    "name": "banco-next-UIclone",
    "slug": "banco-next-UIclone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "splash": {
      "image": "./src/assets/logoName.png",
      "resizeMode": "contain",
      "backgroundColor": "#23FE60"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#23FE60"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
